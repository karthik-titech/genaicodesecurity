{
  "name": "google-home-security-patch",
  "version": "1.0.0",
  "description": "Cybersecurity patch for Google Home devices to prevent malicious prompt injection attacks",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo 'Build completed' && npm run lint && npm test",
    "test": "jest",
    "test-security": "node test-security-patch.js",
    "lint": "eslint src/",
    "security-scan": "npm audit && snyk test",
    "security-audit": "npm audit --audit-level=moderate",
    "format": "prettier --write 'src/**/*.js'",
    "prepare": "husky install"
  },
  "keywords": [
    "google-home",
    "security",
    "prompt-injection",
    "cybersecurity",
    "smart-home"
  ],
  "author": "Cybersecurity Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "sanitize-html": "^2.11.0",
    "xss": "^1.0.14",
    "sqlite3": "^5.1.6",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
